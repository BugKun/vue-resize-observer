{
  "name": "vue-resize-observer",
  "version": "1.0.2",
  "description": "Vue普通元素resize事件监听",
  "main": "dist/vue-resize-observer.js ",
  "keywords": [
    "vue",
    "dom",
    "resize",
    "event",
    "observer"
  ],
  "scripts": {
    "build": "node_modules/.bin/rollup -m -c build/rollup.config.prod.js && uglifyjs dist/vue-resize-observer.js -c -m > dist/vue-resize-observer.min.js",
    "dev": "node build/dev.js",
    "doc": "./node_modules/.bin/jsdoc ./README.md -c ./jsdoc.json",
    "format:src": "prettier-eslint --write \"src/**/*.html\" \"src/**/*.scss\" \"src/**/*.js\" \"src/**/*.vue\"",
    "format:build": "prettier-eslint --write \"build/**/*.html\" \"build/**/*.scss\" \"build/**/*.js\" \"build/**/*.vue\"",
    "format:example": "prettier-eslint --write \"example/**/*.html\" \"example/**/*.scss\" \"example/**/*.js\" \"example/**/*.vue\"",
    "format:test": "prettier-eslint --write \"test/**/*.html\" \"test/**/*.scss\" \"test/**/*.js\" \"test/**/*.vue\"",
    "format": "npm run format:src & npm run format:build & npm run format:example & npm run format:test"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "buble": "^0.19.8",
    "buble-loader": "^0.5.1",
    "css-loader": "^3.4.1",
    "docdash": "^1.1.1",
    "hammerjs": "^2.0.8",
    "jsdoc": "^3.6.3",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "rollup": "^1.28.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "style-loader": "^1.1.2",
    "uglify-js": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.1"
  },
  "author": "wangweiwei",
  "license": "MIT"
}
